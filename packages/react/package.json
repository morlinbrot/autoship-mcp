{
  "name": "@autoship/react",
  "version": "0.2.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "bin": {
    "autoship": "./dist/cli/autoship.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && npm run copy-migrations",
    "copy-migrations": "mkdir -p dist/migrations && cp ../../supabase/migrations/*.sql dist/migrations/",
    "dev": "tsc --watch",
    "cli": "node dist/cli/autoship.js"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.90.1",
    "postgres": "^3.4.5"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.0",
    "react": "^18.3.0",
    "typescript": "^5.8.0"
  },
  "keywords": [
    "autoship",
    "react",
    "feedback",
    "claude",
    "ai",
    "supabase"
  ],
  "license": "MIT"
}
